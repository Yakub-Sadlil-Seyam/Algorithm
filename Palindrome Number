#include<bits/stdc++.h>
using namespace std;
    int main()
    {
        int n,sum = 0;
        cin>>n;
        int temp = n;

        while(temp !=0)
        {
            int t = temp % 10;
            sum = sum*10 + t;
            temp = temp / 10;
        }

        if(n == sum) cout<<"Palindrome"<<endl;
        else cout<<"Not Palindrome"<<endl;
    }
